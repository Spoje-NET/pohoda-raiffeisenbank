{
    "name": "vitexsoftware/pohoda-raiffeisenbank",
    "description": "raiffeisenbank bank statement downloader for Stormware Pohoda",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require": {
        "vgrem/php-spo": "^3",
        "deb/ease-core": "*",
        "deb/rbczpremiumapi": "*",
        "deb/pohoda-sql": "*",
        "deb/pohoda-connector": "*"
    },
    "autoload": {
        "psr-4": {
            "Pohoda\\RaiffeisenBank\\": "/usr/lib/pohoda-raiffeisenbank/Pohoda/RaiffeisenBank/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/EaseCore"
        },
        {
            "type": "path",
            "url": "/usr/share/php/Raiffeisenbank"
        },
        {
            "type": "path",
            "url": "/usr/share/php/GuzzleHttp"
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseFluentPDO/"
        },
        {
            "type": "path",
            "url": "/usr/share/php/PohodaSQL"
        },
        {
            "type": "path",
            "url": "/usr/share/php/mServer"
        }
    ]
}
